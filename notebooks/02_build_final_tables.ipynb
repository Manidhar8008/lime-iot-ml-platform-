df = pd.read_csv("../data/cleaned/bike_cleaned.csv")

fact_trips = (
    df.groupby(["date", "hour"])
    .agg(
        total_trips=("rented_bike_count", "sum"),
        avg_demand=("rented_bike_count", "mean"),
        demand_variance=("rented_bike_count", "var")
    )
    .reset_index()
)

fact_trips.to_csv("../data/final/fact_trips.csv", index=False)
